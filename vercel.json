{
  "version": 2,
  "builds": [
    {
      "src": "server/api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/api/app.py"
    },
    {
      "src": "(.*)",
      "dest": "src/main.jsx"
    }
  ]
}